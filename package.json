  "license": "The MIT License (MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/and-who/react-p5-wrapper.git"
  },
  "bugs": {
    "url": "https://github.com/and-who/react-p5-wrapper/issues"
  },
  "dependencies": {
    "p5": "^1.1.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@types/p5": "^0.9.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^4.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.2.0",
    "source-map-loader": "^1.1.3",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "peerDependencies": {
    "p5": "*",
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack serve --config webpack.config.demo.js --env development --mode development",
    "build": "npm run clean && tsc",
    "build-demo": "npm run clean && webpack --config webpack.config.demo.js --mode production",
    "clean": "rimraf dist",
    "deploy": "gh-pages -d dist",
    "publish-demo": "npm run build-demo && npm run deploy",
    "publish-npm-package": "npm run build && npm publish",
    "format": "prettier --config .prettierrc --write '**/*.{js,jsx,ts,tsx,md}'"
  },
  "lint-staged": {
    "*.tsx": [
      "npm run format",
      "git add"
    ]
  },
  "keywords": [
    "react",
    "react-component",
    "p5",
    "processing"
  ]
}
